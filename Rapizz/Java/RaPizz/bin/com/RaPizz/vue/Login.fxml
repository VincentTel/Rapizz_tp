<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="login_AnchorPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.RaPizz.controleur.LoginControleur">
   <children>
      <VBox alignment="CENTER" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <ImageView fx:id="logo_ImageView" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
            
            <TextField fx:id="Username_TextField" alignment="CENTER" maxWidth="200.0" promptText="Username">
               <VBox.margin>
                  <Insets bottom="5.0" top="20.0" />
               </VBox.margin>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets></TextField>
            <TextField fx:id="Password_TextField" alignment="CENTER" maxWidth="200.0" promptText="Password">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin></TextField>
            <Button fx:id="Connect_Button" alignment="CENTER" mnemonicParsing="false" text="Connect">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin></Button>
            <Button fx:id="Signup_Button" alignment="CENTER" mnemonicParsing="false" text="Signup" />
         </children>
      </VBox>
   </children>
</AnchorPane>
