<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="Header_AnchorPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.RaPizz.controleur.HeaderControleur">
   <children>
      <HBox fx:id="header_HBox" prefHeight="100.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <ImageView fx:id="logo_ImageView" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
            <HBox alignment="CENTER" spacing="10.0">
               <children>
                  <Button fx:id="Ingredients_button" layoutX="139.0" layoutY="10.0" mnemonicParsing="false" text="Manage Ingredients" />
                  <Button fx:id="Pizza_button" mnemonicParsing="false" text="Manage Pizza" />
                  <Button fx:id="Client_button" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" text="Manage Clients" />
                  <Button fx:id="Livreur_button" layoutX="99.0" layoutY="10.0" mnemonicParsing="false" text="Manage Livreur" />
                  <Button fx:id="Liraison_button" layoutX="133.0" layoutY="10.0" mnemonicParsing="false" text="Manage Livraison" />
                  <Button fx:id="Vehicule_button" layoutX="419.0" layoutY="10.0" mnemonicParsing="false" text="Manage Vehicule" />
               </children>
               <padding>
                  <Insets left="2.0" />
               </padding>
            </HBox>
         </children>
         <padding>
            <Insets left="45.0" top="20.0" />
         </padding>
      </HBox>
   </children>
</AnchorPane>
