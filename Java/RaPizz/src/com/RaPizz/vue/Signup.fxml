<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="Signup_AnchorPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.RaPizz.controleur.SignupControleur">
   <children>
    <VBox alignment="CENTER" spacing="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
           <children>
            <ImageView fx:id="Profil_ImageView" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
               
            </ImageView>
            <TextField fx:id="Username_TextField" alignment="CENTER" maxWidth="200.0" promptText="Nom d'utilisateur">
              
            </TextField>
            <TextField fx:id="Password_TextField" alignment="CENTER" maxWidth="200.0" promptText="Mot de passe">
               
            </TextField>
            <TextField fx:id="LastName_TextField" alignment="CENTER" maxWidth="200.0" promptText="Nom de famille">
              
            </TextField>
            <TextField fx:id="FirstName_TextField" alignment="CENTER" maxWidth="200.0" promptText="Prénom">
               
            </TextField>
            <TextField fx:id="Email_TextField" alignment="CENTER" maxWidth="200.0" promptText="Email">
              
            </TextField>
            <TextField fx:id="Number_TextField" alignment="CENTER" maxWidth="200.0" promptText="Numéro de rue">
               
            </TextField>
            <TextField fx:id="Street_TextField" alignment="CENTER" maxWidth="200.0" promptText="Rue">
              
            </TextField>
            <TextField fx:id="City_TextField" alignment="CENTER" maxWidth="200.0" promptText="Ville">
               
            </TextField>
            <TextField fx:id="Zip_TextField" alignment="CENTER" maxWidth="200.0" promptText="Code postal">
               
            </TextField>
            <Button fx:id="Register_Button" mnemonicParsing="false" text="Inscription" />
              <Button fx:id="Cancel_Button" mnemonicParsing="false" text="Annuler" />
         </children>
      </VBox>
   </children>
</AnchorPane>
