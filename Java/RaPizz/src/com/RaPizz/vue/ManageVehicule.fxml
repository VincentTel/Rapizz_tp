            </VBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </left>
   <center>
      <TableView fx:id="Vehicule_TableView" prefHeight="390.0" prefWidth="387.0" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn fx:id="Immat_TableColumn" text="Immatriculation" />
            <TableColumn fx:id="Marque_TableColumn" text="Marque" />
            <TableColumn fx:id="Modele_TableColumn" text="Modèle" />
            <TableColumn fx:id="Type_TableColumn" text="Type" />
            <TableColumn fx:id="Action_TableColumn" text="Action" />            
            <TableColumn fx:id="ActionUpdate_TableColumn" text="Action" />
        </columns> 
        <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </center>
</BorderPane>
