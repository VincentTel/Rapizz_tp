<?import javafx.scene.layout.*?>

<BorderPane fx:id="ManageVehicule_BorderPane" prefHeight="351.0" prefWidth="577.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.RaPizz.controleur.ManageVehiculeControleur">
   <left>
      <VBox prefHeight="384.0" prefWidth="195.0" spacing="5.0" BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="Immat_TextField" promptText="Immatriculation" />
            <TextField fx:id="Marque_TextField" promptText="Marque" />
            <TextField fx:id="Modele_TextField" promptText="Modèle" />
            <TextField fx:id="Type_TextField" promptText="Type" />
            <VBox prefHeight="334.0" spacing="7.0"  prefWidth="195.0">
               <children>
                  <Button fx:id="Add_Button" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Ajouter" />
                  <Button fx:id="Update_Button"  maxWidth="1.7976931348623157E308" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" text="Update" />
                  <Button fx:id="Cancel_Button"  maxWidth="1.7976931348623157E308" alignment="CENTER" contentDisplay="CENTER" layoutX="65.0" layoutY="10.0" mnemonicParsing="false" text="Cancel" />
